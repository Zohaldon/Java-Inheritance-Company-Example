
public class Employee {

	//Fields
	String name;
	long salary;
	
	//Constructor
	public Employee(String name, long salary) 
	{
		super();
		this.name = name;
		this.salary = salary;
	}
	
	//Properties
	public String getName()
	{
		return name;
	}
	public void setName(String name)
	{
		this.name = name;
	}

	public long getSalary() 
	{
		return salary;
	}
	public void setSalary(long salary) 
	{
		this.salary = salary;
	}
	
	@Override
	public String toString()
	{
		return "The Name of the Employee is " + name + "\nThe Salary of " + name + " is: " + salary;
	}
		
}
